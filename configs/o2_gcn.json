{
    "model": "GCN",
    "dataset": "o2",
    
    "out_dir": "baseline_res/regression/",

    "params": {
        "train_repeat_time":15,
        "use_concat_train":true,
        "seed": 2,
        "epochs": 400,
        "batch_size": 32,
        "init_lr": 1e-3,
        "patience":100,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "L": 4,
        "hidden_dim": 300,
        "out_dim": 300,
        "residual": true,
        "readout": "mean",
        "in_feat_dropout": 0.0,
        "dropout": 0.1,
        "batch_norm": true,
        "edge_feat": true,
        "pos_enc": false,
        "pos_enc_dim": 8
    }
}